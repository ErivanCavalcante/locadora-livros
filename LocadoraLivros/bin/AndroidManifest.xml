<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcc.locadoralivros"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="Locadora de Livros" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NovoLoginActivity"
            android:label="Novo Login" >
        </activity>
        <activity
            android:name=".ListaActivity"
            android:label="Lista Livros" >
        </activity>
        <activity
            android:name=".AlugarActivity"
            android:label="Alugar" >
        </activity>

        <activity
            android:name=".DadosPessoaisActivity"
            android:label="Dados Pessoais" >
        </activity>
        <activity
            android:name=".ListaLivrosUsuActivity"
            android:label="Meus Livros" >
        </activity>
        <activity
            android:name=".CadastroLivroActivity"
            android:label="Cadastrar Livro" >
        </activity>
    </application>

</manifest>
